{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/login",
        "dest": "/api/login.js"
      },
      {
        "src": "/api/signup",
        "dest": "/api/signup.js"
      },
      {
        "src": "/(.*)",
        "dest": "/api/views.js"
      }
    ]
  }